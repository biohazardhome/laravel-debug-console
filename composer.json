{
  "name": "biohazard/laravel-debug-console",
  "description": "Works as a console viewer for laravel debugbar.",
  "keywords": ["laravel", "debugbar", "debug", "console"],
  "license": "MIT",
  "authors": [
    {
      "name": "biohazard",
      "email": "stalker-nikko@yandex.ru"
    }
  ],
  "require": {
    "php": ">=8.2",
    "illuminate/console": "^10.7.1",
    "barryvdh/laravel-debugbar": "^3.8.1",
    "clue/stdio-react": "^2.6.0"
  },
  "require-dev": {
    "madewithlove/php-cs-fixer-config": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Madewithlove\\LaravelDebugConsole\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Madewithlove\\LaravelDebugConsole\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "lint": "php-cs-fixer fix"
  }
}
